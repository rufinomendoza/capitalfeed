- if admin_signed_in?
  %h1 Admins
  = will_paginate @admins, class: 'musings-pagination'
  %table
    %thead
      %th E-Mail
      %th God Mode
      %th Reports Only
      %th Sign In Count
      %th Current Sign In At
      %th Last Sign In At
      %th Current Sign In IP
      %th Last Sign In IP
      %th Created At
      %th Updated At
      - if current_admin.god_mode?
        %th
    %tbody
      - for admin in @admins
        %tr
          %td= admin.email
          %td= admin.god_mode
          %td= admin.reports_only
          %td= admin.sign_in_count
          %td= admin.current_sign_in_at
          %td= admin.last_sign_in_at
          %td= admin.current_sign_in_ip
          %td= admin.last_sign_in_ip
          %td= admin.created_at
          %td= admin.updated_at
          - if current_admin.god_mode?
            %td= link_to 'Delete', admin, method: :delete, data: { confirm: 'Are you sure?' }
  = will_paginate @admins, class: 'musings-pagination'
  %br/
  %h2 Users
  = will_paginate @users, class: 'musings-pagination'
  %table
    %thead
      %th E-Mail
      %th Sign In Count
      %th Current Sign In At
      %th Last Sign In At
      %th Current Sign In IP
      %th Last Sign In IP
      %th Created At
      %th Updated At
      - if current_admin.god_mode?
        %th
    %tbody
      - for user in @users
        %tr
          %td= user.email
          %td= user.sign_in_count
          %td= user.current_sign_in_at
          %td= user.last_sign_in_at
          %td= user.current_sign_in_ip
          %td= user.last_sign_in_ip
          %td= user.created_at
          %td= user.updated_at
          - if current_admin.god_mode?
            %td= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?' }
  = will_paginate @users, class: 'musings-pagination'
